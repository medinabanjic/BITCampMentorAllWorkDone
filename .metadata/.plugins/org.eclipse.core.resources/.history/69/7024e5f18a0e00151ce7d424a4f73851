package ba.bitcamp.dreamteam;

public class Medina {

	public static void getValue(String[] valute, double[] value){
		
		int counter = 0;
		TextIO.readFile("src/Currency.txt");
		while (!TextIO.eof()){
		
			TextIO.getln();
			counter++;
			
		}
		
		for (int i = 0; i <= counter ; i++){
			
			String valuteName = TextIO.getln();
			String[] parts = valuteName.split(" ");
			String part1 = parts[0];
			
			valute[i] = part1;
			
			for(String value1 : valute){
				
				System.out.println(value1);
			}
		}
		
	}
	
	public static void main(String[] args) {
		
		String[] valute = new String[9];
		int counter = 0;
		TextIO.readFile("src/Currency.txt");
		while (!TextIO.eof()){
		
			TextIO.getln();
			counter++;
			
		}
		
		for (int i = 0; i <= counter ; i++){
			
			String valuteName = TextIO.getln();
			String[] parts = valuteName.split(" ");
			String part1 = parts[0];
			
			valute[i] = part1;
			
			for(String value1 : valute){
				
				System.out.println(value1);
			}
		}
	}

}
