package ba.bitcamp.exercises.ass3;

public class Main {

	public static void main(String[] args) {
		Printer p1 = new Printer();
		Printer p2 = new Printer("Moj licni printer");

		
		System.out.println("First printer has tint: " + p1.hasTint() 
				+ " \nSecond has tint: " + p2.hasTint());
	
		System.out.println("First printer has paper: " + p1.hasPaper() 
				+ " \nSecond has paper: " + p2.hasPaper());
		
		System.out.println("First printer printed totally: " + p1.getPrintedPages());
	
		System.out.println("Second printer printed totally: " + p2.getPrintedPages());
		
		
		p1.addPaper(102);
		p2.addPaper(54);
		
		p1.addMaxTint();
		p2.addMaxTint();
		
		System.out.println("First printer has tint: " + p1.hasTint() 
				+ " \nSecond has tint: " + p2.hasTint());
	
		System.out.println("First printer has paper: " + p1.hasPaper() 
				+ " \nSecond has paper: " + p2.hasPaper());
		
		
		
	}
}
